for a = 1, 3, 1 do
    local number = 0
    local random = math.random(1, 20)
    local randomness = 10

    function random1(failed, success)
        if randomness <= random then
            failed = number - 1
            print(failed, "failed")
        else
            success = number + 1
            print(success, "success")
        end
    end
    random1()
end
-- this is just a really old test script for a fnaf game xd
-- this is back when i was learning lua
-- i was trying to make a rng for the animatronic ai
